<h2><%%= @title %></h2>
<%%= f = form_for @changeset, "#",
  id: "<%= schema.singular %>-form",
class: "form",
phx_target: @myself,
phx_change: "validate",
phx_submit: "save" %>
<%= for {label, input, error} <- inputs, input do %>
  <%= label %>
  <%= input %>
  <%= error %>
<% end %>
<div class="mt-10">
  <%%= submit "Save", class: "btn btn--blue", phx_disable_with: "Saving..." %>
</div>
</form>
